<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.9.xsd">
    <!--
        Added the constraints for entity DictionaryHasWord.
    -->
    <changeSet id="20210102145701-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="dictionary_id"
                                 baseTableName="dictionary_has_word"
                                 constraintName="fk_dictionary_has_word_dictionary_id"
                                 referencedColumnNames="id"
                                 referencedTableName="dictionary"/>

        <addForeignKeyConstraint baseColumnNames="word_id"
                                 baseTableName="dictionary_has_word"
                                 constraintName="fk_dictionary_has_word_word_id"
                                 referencedColumnNames="id"
                                 referencedTableName="word"/>

    </changeSet>
</databaseChangeLog>
